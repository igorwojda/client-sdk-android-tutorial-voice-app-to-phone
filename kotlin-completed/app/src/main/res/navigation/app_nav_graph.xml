<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/app_navigation_graph"
        app:startDestination="@id/loginFragment">
    <fragment
            android:id="@+id/loginFragment"
            android:name="com.vonage.tutorial.voice.LoginFragment"
            android:label="LoginFragment"
            tools:layout="@layout/fragment_login">
        <action
                android:id="@+id/action_loginFragment_to_voiceFragment"
                app:destination="@id/voiceFragment" />
    </fragment>
    <fragment
            android:id="@+id/voiceFragment"
            android:name="com.vonage.tutorial.voice.VoiceFragment"
            android:label="VoiceFragment"
            tools:layout="@layout/fragment_voice" />
</navigation>
